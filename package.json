{
  "name": "empty_shell",
  "version": "1.0.0",
  "prettier": {
    "singleQuote": true
  },
  "description": "This package set's up ts, jasmine, eslint, prettier and some other base stuff for reuse",
  "main": "src/index.ts",
  "scripts": {
    "build": "npx tsc",
    "dev": "npm-run-all --parallel watch:build watch:start",
    "watch:build": "tsc --watch",
    "watch:start": "nodemon dist/index.js",
    "jasmine": "jasmine",
    "lint": "eslint src --ext .ts",
    "nodemon": "nodemon src/index.ts",
    "prettier": "prettier --write \"src/**/*.ts\"",
    "test": "npm run build && npm run jasmine"
  },
  "author": "Daniel Leutwyler",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.16",
    "@types/jasmine": "^4.3.1",
    "@types/sharp": "^0.31.1",
    "@types/typescript": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-vue": "^9.9.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.3",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "express": "^4.18.2",
    "jasmine": "^4.5.0",
    "jasmine-spec-reporter": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "sharp": "^0.31.3",
    "ts-node": "^10.9.1"
  }
}
